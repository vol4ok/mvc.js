#CSON

$plugins: 
  $type: "plugin"
  plugins: [ './_build/exec.nbplug/exec.nbplug.coffee', './_build/coffee.nbplug/coffee.nbplug.coffee', './_build/jslink.nbproj/jslink.nbplug.coffee']

build:
  coffee:
    $type: "exec"
    exec: [ "mkdir -p _int", "coffee -b -o _int src" ]
    
  link:
    $type: "jslink"
    src: '_int/mvc.js'
    dst: 'lib'
    compress: no
    wrapNs: yes